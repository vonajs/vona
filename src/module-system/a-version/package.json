  "exports": {
    ".": {
      "types": [
        "./dist/index.d.ts"
      ],
      "import": "./dist/index.js",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "description": "cabloy-module-api-a-version",
