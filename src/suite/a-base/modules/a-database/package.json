{
  "name": "cabloy-module-api-a-database",
  "version": "5.0.0",
  "title": "a-database",
  "eggBornModule": {
    "fileVersion": 0,
    "dependencies": {}
  },
  "type": "module",
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "development": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "description": "db",
  "files": [
    "dist",
    "static",
    "typings"
  ],
  "scripts": {
    "build:front": "node ../../../scripts/egg-born-bin.js front-build-module",
    "build:backend": "node ../../../scripts/egg-born-bin.js backend-build-module",
    "build:all": "npm run build:front && npm run build:backend",
    "preversion": "npm run build:all && git add ."
  },
  "keywords": [
    "Cabloy Module"
  ],
  "author": "zhennann",
  "dependencies": {
    "@cabloy/deps": "workspace:^",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "pg-types": "^4.0.2",
    "mysql2": "^3.9.1"
  }
}
