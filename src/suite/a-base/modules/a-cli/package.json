{
  "name": "vona-module-a-cli",
  "version": "5.0.0",
  "title": "a-cli",
  "cabloyModule": {
    "fileVersion": 0,
    "dependencies": {
      "a-instance": "5.0.0",
      "a-base": "5.0.0"
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "development": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "description": "Cli",
  "files": [
    "dist",
    "static",
    "typings"
  ],
  "scripts": {
    "build:front": "node ../../../scripts/egg-born-bin.js front-build-module",
    "build:backend": "node ../../../scripts/egg-born-bin.js backend-build-module",
    "build:all": "npm run build:front && npm run build:backend",
    "preversion": "npm run build:all && git add ."
  },
  "keywords": [
    "Cabloy Module"
  ],
  "author": "zhennann",
  "dependencies": {
    "@cabloy/process-helper": "workspace:^",
    "@cabloy/module-info": "workspace:^",
    "@zhennann/ejs": "^3.0.1",
    "boxen": "^4.2.0",
    "chalk": "^3.0.0",
    "cli-table3": "^0.6.2",
    "gogocode": "^1.0.53",
    "istextorbinary": "^3.3.0"
  }
}
