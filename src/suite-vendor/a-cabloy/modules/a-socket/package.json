{
  "name": "vona-module-a-socket",
  "type": "module",
  "version": "5.0.0",
  "title": "a-socket",
  "vonaModule": {
    "dependencies": {
      "a-vona": "5.0.0"
    },
    "onions": {
      "socketConnection": {
        "optionsGlobalInterfaceName": "IDecoratorSocketConnectionOptions",
        "optionsGlobalInterfaceFrom": "vona-module-a-socket",
        "boilerplate": "cli/socketConnection/boilerplate"
      },
      "socketPacket": {
        "optionsGlobalInterfaceName": "IDecoratorSocketPacketOptions",
        "optionsGlobalInterfaceFrom": "vona-module-a-socket",
        "boilerplate": "cli/socketPacket/boilerplate"
      }
    },
    "capabilities": {
      "monkey": true
    }
  },
  "description": "",
  "author": "",
  "keywords": [
    "Vona Module"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "cli",
    "dist",
    "static"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "ws": "^8.18.1"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "scripts",
      "exports.\\..import"
    ]
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  }
}
