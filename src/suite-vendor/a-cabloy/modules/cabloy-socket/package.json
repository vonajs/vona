{
  "name": "vona-module-cabloy-socket",
  "type": "module",
  "version": "5.0.0",
  "title": "cabloy-socket",
  "vonaModule": {
    "dependencies": {
      "a-vona": "5.0.0"
    },
    "globalDependencies": {
      "@cabloy/socket": true
    }
  },
  "description": "",
  "author": "",
  "keywords": ["Vona Module"],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist", "static"],
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "scripts",
      "exports.\\..import"
    ]
  },
  "dependencies": {
    "@cabloy/socket": "workspace:^"
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  }
}
