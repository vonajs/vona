{
  "name": "vona-module-a-core",
  "type": "module",
  "version": "5.0.3",
  "title": "a-core",
  "vonaModule": {
    "dependencies": {},
    "globalDependencies": {
      "@cabloy/deps": true,
      "@cabloy/json5": true,
      "@cabloy/module-info:": true,
      "@cabloy/set": true,
      "@cabloy/utils": true,
      "@cabloy/word-utils": true,
      "boxen": true,
      "cel-js": true,
      "chalk": true,
      "cli-table3": true,
      "fs-extra": true,
      "moment": true,
      "retry": true,
      "uuid": true,
      "vona-shared": true
    },
    "globalDependenciesDev": {
      "@cabloy/cli": true,
      "@cabloy/lint": true,
      "@swc/core": true,
      "@swc/helpers": true,
      "@types/node": true,
      "@types/retry": true,
      "eslint": true,
      "ts-node": true,
      "tslib": true,
      "typescript": true,
      "vona-mock": true,
      "why-is-node-running": true
    }
  },
  "description": "",
  "author": "",
  "keywords": [
    "Vona Module"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "static"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@cabloy/deps": "workspace:^",
    "@cabloy/json5": "workspace:^",
    "@cabloy/module-info": "workspace:^",
    "@cabloy/set": "workspace:^",
    "@cabloy/utils": "workspace:^",
    "@cabloy/word-utils": "^2.0.0",
    "boxen": "^8.0.1",
    "cel-js": "^0.3.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "fs-extra": "^11.2.0",
    "koa-bodyparser": "^4.4.1",
    "koa-override": "^4.0.0",
    "moment": "^2.30.1",
    "retry": "^0.13.1",
    "uuid": "^11.0.3",
    "vona-shared": "workspace:^"
  },
  "devDependencies": {
    "@cabloy/cli": "workspace:^",
    "@cabloy/lint": "workspace:^",
    "@swc/core": "^1.7.36",
    "@swc/helpers": "^0.5.13",
    "@types/node": "^22.10.2",
    "@types/retry": "^0.12.5",
    "clean-package": "^2.2.0",
    "eslint": "^9.20.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vona-mock": "workspace:^",
    "why-is-node-running": "^3.2.2"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "scripts",
      "exports.\\..import"
    ]
  }
}
