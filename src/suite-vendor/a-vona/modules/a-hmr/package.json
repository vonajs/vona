{
  "name": "vona-module-a-hmr",
  "type": "module",
  "version": "5.0.0",
  "title": "a-hmr",
  "vonaModule": {
    "capabilities": {
      "meta": {
        "mode": [
          "dev"
        ]
      },
      "monkey": true
    },
    "dependencies": {}
  },
  "description": "",
  "author": "",
  "keywords": [
    "Vona Module"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "assets",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.build.tsbuildinfo",
    "tsc:publish": "npm run clean && vona :bin:buildModule && tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "exports.\\..import"
    ]
  },
  "dependencies": {
    "chokidar": "^5.0.0",
    "debounce": "^3.0.0",
    "globby": "^16.0.0"
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  }
}
