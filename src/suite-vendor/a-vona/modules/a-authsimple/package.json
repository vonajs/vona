{
  "name": "vona-module-a-authsimple",
  "type": "module",
  "version": "5.0.15",
  "title": "a-authsimple",
  "vonaModule": {
    "fileVersion": 1,
    "dependencies": {
      "a-auth": "5.0.0",
      "home-user": "5.0.0"
    }
  },
  "description": "",
  "author": "",
  "keywords": [
    "Vona Module"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "static"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.build.tsbuildinfo",
