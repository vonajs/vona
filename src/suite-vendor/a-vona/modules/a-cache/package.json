{
  "name": "vona-module-a-cache",
  "type": "module",
  "version": "5.0.11",
  "title": "a-cache",
  "vonaModule": {
    "dependencies": {},
    "onions": {
      "cacheMem": {
        "optionsGlobalInterfaceName": "IDecoratorCacheMemOptions",
        "optionsGlobalInterfaceFrom": "vona-module-a-cache",
        "scopeResource": true,
        "boilerplate": "cli/cacheMem/boilerplate"
      },
      "cacheRedis": {
        "optionsGlobalInterfaceName": "IDecoratorCacheRedisOptions",
        "optionsGlobalInterfaceFrom": "vona-module-a-cache",
        "scopeResource": true,
        "boilerplate": "cli/cacheRedis/boilerplate"
      }
    }
  },
  "description": "",
  "author": "",
  "keywords": [
    "Vona Module"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "static"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.build.tsbuildinfo",
    "tsc:publish": "npm run clean && vona :bin:buildModule && tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "lru-cache": "^11.0.2",
    "object-hash": "^3.0.0"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "exports.\\..import"
    ]
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  }
}
