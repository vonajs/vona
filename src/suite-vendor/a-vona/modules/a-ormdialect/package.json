{
  "name": "vona-module-a-ormdialect",
  "type": "module",
  "version": "5.0.25",
  "title": "a-ormdialect",
  "vonaModule": {
    "dependencies": {},
    "globalDependencies": {
      "better-sqlite3": true
    }
  },
  "description": "",
  "author": "",
  "keywords": [
    "Vona Module"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "assets",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.build.tsbuildinfo",
    "tsc:publish": "npm run clean && vona :bin:buildModule && tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore && npm run clean"
  },
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "globby": "^16.0.0",
    "mysql2": "^3.15.0",
    "pg": "^8.16.3",
    "pg-types": "^4.1.0"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "exports.\\..import"
    ]
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  }
}
