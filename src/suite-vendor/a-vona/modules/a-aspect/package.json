{
  "name": "vona-module-a-aspect",
  "type": "module",
  "version": "5.0.18",
  "title": "a-aspect",
  "vonaModule": {
    "dependencies": {},
    "onions": {
      "filter": {
        "hasLocal": true,
        "optionsRoute": true,
        "optionsDynamic": true,
