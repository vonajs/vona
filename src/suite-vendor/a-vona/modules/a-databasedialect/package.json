{
  "name": "vona-module-a-databasedialect",
  "type": "module",
  "version": "5.0.0",
  "title": "a-databasedialect",
  "vonaModule": {
    "dependencies": {}
  },
  "description": "",
  "author": "",
  "keywords": [
    "Vona Module"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "static"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "peerDependencies": {
    "better-sqlite3": "^11.8.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.12.0",
    "oracledb": "^6.7.2",
    "pg": "^8.13.3",
    "pg-native": "^3.2.2",
    "pg-query-stream": "^4.7.3",
    "pg-types": "^4.0.2",
    "sqlite3": "^5.1.7",
    "tedious": "^18.6.1"
  },
  "dependencies": {},
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "scripts",
      "exports.\\..import"
    ]
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  }
}
