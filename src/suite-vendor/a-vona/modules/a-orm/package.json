{
  "name": "vona-module-a-orm",
  "type": "module",
  "version": "5.0.36",
  "title": "a-orm",
  "vonaModule": {
    "capabilities": {
      "monkey": true
    },
    "dependencies": {},
    "globalDependencies": {
      "bignumber.js": true,
      "knex": true,
      "table-identity": true
    },
    "onions": {
      "entity": {
        "sceneIsolate": true,
        "optionsGlobalInterfaceName": "IDecoratorEntityOptions",
        "optionsGlobalInterfaceFrom": "vona-module-a-orm",
        "scopeResource": true,
        "scopeResourceTypeTemplate": "{{className}}Meta",
        "boilerplate": "cli/entity/boilerplate",
        "metadataCustom": "cli/entity/metadata/generate.ts"
      },
      "model": {
        "sceneIsolate": true,
        "optionsGlobalInterfaceName": "IDecoratorModelOptions",
        "optionsGlobalInterfaceFrom": "vona-module-a-orm",
        "scopeResource": true,
        "boilerplate": "cli/model/boilerplate",
        "metadataCustom": "cli/model/metadata/generate.ts"
      },
      "databaseDialect": {
        "beanGeneral": true,
        "boilerplate": "cli/databaseDialect/boilerplate"
      }
    }
  },
  "description": "db",
  "author": "zhennann",
  "keywords": [
    "Cabloy Module"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "cli",
    "dist",
    "static"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.build.tsbuildinfo",
    "tsc:publish": "npm run clean && vona :bin:buildModule && tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "knex": "^3.1.0",
    "table-identity": "^1.0.10"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "exports.\\..import"
    ]
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  },
  "gitHead": "0eab9dc4a5622caffe89e7b1b3f02c08ccbc4c4b"
}
