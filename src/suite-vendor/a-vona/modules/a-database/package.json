{
  "name": "vona-module-a-database",
  "version": "5.0.7",
  "title": "a-database",
  "private": true,
  "vonaModule": {
    "capabilities": {
      "monkey": true
    },
    "dependencies": {},
    "globalDependencies": {
      "bignumber.js": true,
      "knex": true
    },
    "onions": {
      "entity": {
        "sceneIsolate": true,
        "optionsGlobalInterfaceName": "IDecoratorEntityOptions",
        "optionsGlobalInterfaceFrom": "vona-module-a-database",
        "scopeResource": true,
        "boilerplate": "cli/entity/boilerplate",
        "metadataCustom": "cli/entity/metadata/generate.ts"
      },
      "model": {
        "sceneIsolate": true,
        "optionsGlobalInterfaceName": "IDecoratorModelOptions",
        "optionsGlobalInterfaceFrom": "vona-module-a-database",
        "scopeResource": true,
        "boilerplate": "cli/model/boilerplate"
      }
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "development": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "description": "db",
  "files": [
    "dist",
    "static",
    "cli"
  ],
  "scripts": {},
  "keywords": [
    "Cabloy Module"
  ],
  "author": "zhennann",
  "dependencies": {
    "@cabloy/deps": "workspace:^",
    "bignumber.js": "^9.1.2",
    "knex": "^3.1.0",
    "mysql2": "^3.9.1",
    "pg": "^8.11.3",
    "pg-types": "^4.0.2"
  }
}
