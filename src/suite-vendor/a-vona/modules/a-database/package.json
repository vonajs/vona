{
  "name": "vona-module-a-database",
  "type": "module",
  "version": "5.0.8",
  "title": "a-database",
  "private": true,
  "vonaModule": {
    "capabilities": {
      "monkey": true
    },
    "dependencies": {},
    "globalDependencies": {
      "bignumber.js": true,
      "knex": true,
      "table-identity": true
    },
    "onions": {
      "entity": {
        "sceneIsolate": true,
        "optionsGlobalInterfaceName": "IDecoratorEntityOptions",
        "optionsGlobalInterfaceFrom": "vona-module-a-database",
        "scopeResource": true,
        "boilerplate": "cli/entity/boilerplate",
        "metadataCustom": "cli/entity/metadata/generate.ts"
      },
      "model": {
        "sceneIsolate": true,
        "optionsGlobalInterfaceName": "IDecoratorModelOptions",
        "optionsGlobalInterfaceFrom": "vona-module-a-database",
        "scopeResource": true,
        "boilerplate": "cli/model/boilerplate"
      },
      "databaseDialect": {
        "beanGeneral": true,
        "boilerplate": "cli/databaseDialect/boilerplate"
      }
    }
  },
  "description": "db",
  "author": "zhennann",
  "keywords": [
    "Cabloy Module"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "cli",
    "dist",
    "static"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "knex": "^3.1.0",
    "table-identity": "workspace:^"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "scripts",
      "exports.\\..import"
    ]
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  }
}
