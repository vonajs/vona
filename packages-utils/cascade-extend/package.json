{
  "name": "cascade-extend",
  "type": "module",
  "version": "2.1.1",
  "description": "cascade-extend",
  "publishConfig": {
    "access": "public"
  },
  "author": "zhennann",
  "license": "MIT",
  "homepage": "https://github.com/zhennann/cascade-extend#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhennann/cascade-extend.git"
  },
  "bugs": {
    "url": "https://github.com/zhennann/cascade-extend/issues"
  },
  "keywords": [
    "cabloy",
    "cascade-extend"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "clean": "rimraf dist tsconfig.build.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "exports.\\..import"
    ]
  },
  "dependencies": {
    "@cabloy/extend": "workspace:^"
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  }
}
