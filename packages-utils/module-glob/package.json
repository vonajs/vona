{
  "name": "@cabloy/module-glob",
  "type": "module",
  "version": "5.2.25",
  "description": "cabloy module-glob",
  "publishConfig": {
    "access": "public"
  },
  "author": "zhennann",
  "keywords": [
    "framework",
    "cabloy"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@cabloy/module-info": "^1.3.26",
    "@cabloy/utils": "^1.0.40",
    "boxen": "^4.2.0",
    "chalk": "^3.0.0",
    "fs-extra": "^11.3.0",
    "globby": "^14.1.0",
    "semver": "^7.7.1"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "exports.\\..import"
    ]
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  },
  "gitHead": "0eab9dc4a5622caffe89e7b1b3f02c08ccbc4c4b"
}
