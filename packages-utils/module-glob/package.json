    "egg",
    "egg-born",
    "framework",
    "cabloy"
  ],
  "author": "zhennann",
  "license": "MIT",
  "dependencies": {
    "@cabloy/module-info": "workspace:^",
    "egg-born-utils": "^1.1.9"
  }
}
