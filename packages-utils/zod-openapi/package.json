{
  "name": "@cabloy/zod-openapi",
  "type": "module",
  "version": "1.0.0",
  "description": "zod-openapi",
  "publishConfig": {
    "access": "public"
  },
  "author": "zhennann",
  "license": "MIT",
  "homepage": "https://github.com/cabloy/zova#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabloy/zova.git"
  },
  "bugs": {
    "url": "https://github.com/cabloy/zova/issues"
  },
  "keywords": [
    "vue3",
    "ioc",
    "zova"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "exports.\\..import"
    ]
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  }
}
