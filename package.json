{
  "name": "cabloy-api",
  "title": "Cabloy",
  "private": true,
  "type": "commonjs",
  "description": "A Node.js full-stack framework with workflow engine, based on koa + egg + vue + framework7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhennann/cabloy.git"
  },
  "author": "zhennann",
  "keywords": [
    "nodejs",
    "full-stack",
    "workflow-engine",
    "egg-framework",
    "framework7",
    "cabloy"
  ],
  "bugs": {
    "url": "https://github.com/zhennann/cabloy/issues"
  },
  "homepage": "https://github.com/zhennann/cabloy#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "cabloy": "cabloy",
    "cli": "node ./scripts/egg-born-bin.js cli",
    "db:reset": "node ./scripts/egg-born-bin.js backend-db-reset",
    "dev": "node ./scripts/egg-born-bin.js backend-dev --workers=2",
    "dev:one": "node ./scripts/egg-born-bin.js backend-dev --workers=1",
    "test": "node ./scripts/egg-born-bin.js backend-test",
    "cov": "node ./scripts/egg-born-bin.js backend-cov",
    "build": "node ./scripts/egg-born-scripts.js backend-build",
    "start": "node ./scripts/egg-born-scripts.js backend-start",
    "start:daemon": "node ./scripts/egg-born-scripts.js backend-start --daemon",
    "stop": "node ./scripts/egg-born-scripts.js backend-stop",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --fix --ext .ts src"
  },
  "pnpm": {
    "overrides": {
      "@types/mime": "3.0.4"
    }
  },
  "dependencies": {
    "@cabloy/core": "workspace:^"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.4",
    "@babel/register": "^7.23.7",
    "@cabloy/extend": "workspace:^",
    "@cabloy/lint": "workspace:^",
    "@cabloy/module-glob": "workspace:^",
    "@cabloy/process-helper": "workspace:^",
    "@swc/core": "^1.5.1",
    "@swc/helpers": "^0.5.11",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.19.3",
    "@zhennann/framework7": "^5.8.2",
    "@zhennann/socketio": "^4.0.2",
    "async": "^3.2.5",
    "autoprefixer": "^9.8.8",
    "axios": "^1.6.8",
    "bagpipe": "^0.3.5",
    "bluebird": "^3.7.2",
    "chalk": "^3.0.0",
    "commander": "^11.1.0",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^7.0.0",
    "egg-born-mock": "workspace:^",
    "egg-born-utils": "^1.1.9",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.19.2",
    "extend2": "^1.0.1",
    "fs-extra": "^10.1.0",
    "function-bind": "^1.1.2",
    "glob": "^10.3.12",
    "gogocode": "^1.0.55",
    "http-proxy-middleware": "^1.3.1",
    "is-type-of": "^1.4.0",
    "js-cookie": "^2.2.1",
    "jsdoc": "^4.0.2",
    "json5": "^2.2.3",
    "less": "^3.13.1",
    "mini-css-extract-plugin": "^1.6.2",
    "minimatch": "^9.0.4",
    "moment": "^2.30.1",
    "npm-request": "^1.0.0",
    "open": "^7.4.2",
    "ora": "^4.1.1",
    "p-map": "^3.0.0",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^5.0.5",
    "semver": "^7.6.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.3",
    "typedoc": "^0.25.13",
    "typescript": "5.3.3",
    "vue-eslint-parser": "^9.4.2"
  }
}
