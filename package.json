{
  "name": "cabloy-source",
  "title": "Cabloy",
  "private": true,
  "type": "commonjs",
  "description": "A Node.js full-stack framework with workflow engine, based on koa + egg + vue + framework7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhennann/cabloy.git"
  },
  "author": "zhennann",
  "license": "MIT",
  "keywords": [
    "nodejs",
    "full-stack",
    "workflow-engine",
    "egg-framework",
    "framework7",
    "cabloy"
  ],
  "bugs": {
    "url": "https://github.com/zhennann/cabloy/issues"
  },
  "homepage": "https://github.com/zhennann/cabloy#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "cli": "node ./scripts/egg-born-bin.js cli",
    "db:reset": "node ./scripts/egg-born-bin.js backend-db-reset",
    "dev:front": "node ./scripts/egg-born-bin.js front-dev",
    "build:front": "node ./scripts/egg-born-bin.js front-build",
    "dev:backend": "node ./scripts/egg-born-bin.js backend-dev",
    "debug:backend": "node ./scripts/egg-born-bin.js backend-debug",
    "test": "npx mocha",
    "cov:backend": "node ./scripts/egg-born-bin.js backend-cov",
    "start:backend": "node ./scripts/egg-born-scripts.js backend-start",
    "start:backend-daemon": "node ./scripts/egg-born-scripts.js backend-start --daemon",
    "stop:backend": "node ./scripts/egg-born-scripts.js backend-stop",
    "commits": "./scripts/commits.sh",
    "commits-all": "./scripts/commits-all.sh",
    "format": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.jsx\" \"**/*.vue\"",
    "lint": "prettier --check \"**/*.js\" \"**/*.json\" \"**/*.jsx\" \"**/*.vue\"",
    "eslint": "eslint --ext .js,.json,.jsx,.vue src",
    "cnpm:sync": "node ./scripts/cnpm-sync.js"
  },
  "dependencies": {
    "@cabloy/core": "workspace:^"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "@cabloy/extend": "^3.0.2",
    "@cabloy/module-glob": "workspace:^",
    "@cabloy/process-helper": "workspace:^",
    "@swc/core": "^1.3.107",
    "@swc/helpers": "^0.5.3",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.19.3",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@zhennann/framework7": "^5.8.2",
    "@zhennann/socketio": "^4.0.2",
    "async": "^3.2.0",
    "autoprefixer": "^9.7.4",
    "axios": "^1.6.2",
    "bagpipe": "^0.3.5",
    "bluebird": "^3.7.2",
    "chalk": "^3.0.0",
    "commander": "^11.1.0",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^7.0.0",
    "egg-born-utils": "^1.1.9",
    "egg-born-mock": "workspace:^",
    "eslint": "^8.56.0",
    "eslint-config-egg": "^13.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsdoc": "^46.9.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "extend2": "^1.0.1",
    "fs-extra": "^10.1.0",
    "function-bind": "^1.1.1",
    "glob": "^10.3.10",
    "gogocode": "^1.0.53",
    "http-proxy-middleware": "^1.0.6",
    "is-type-of": "^1.2.0",
    "js-cookie": "^2.2.1",
    "jsdoc": "^4.0.2",
    "json5": "^2.1.1",
    "less": "^3.13.0",
    "mini-css-extract-plugin": "^1.3.3",
    "minimatch": "^9.0.3",
    "mocha": "^10.2.0",
    "moment": "^2.24.0",
    "npm-request": "^1.0.0",
    "open": "^7.0.2",
    "ora": "^4.0.3",
    "p-map": "^3.0.0",
    "prettier": "2.3.2",
    "regenerator-runtime": "^0.14.0",
    "rimraf": "^5.0.5",
    "semver": "^7.3.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typedoc": "^0.25.0",
    "typescript": "5.3.3",
    "vue-eslint-parser": "^9.1.0"
  }
}
