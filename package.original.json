{
  "name": "cabloy-api",
  "title": "Cabloy",
  "private": true,
  "type": "module",
  "description": "A Node.js full-stack framework with workflow engine, based on koa + egg + vue + framework7",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhennann/cabloy.git"
  },
  "author": "zhennann",
  "keywords": ["nodejs", "full-stack", "workflow-engine", "egg-framework", "framework7", "cabloy"],
  "bugs": {
    "url": "https://github.com/zhennann/cabloy/issues"
  },
  "homepage": "https://github.com/zhennann/cabloy#readme",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "vona": "node ./packages-cli/cli/src/bin/vona.ts",
    "cli": "node ./scripts/egg-born-bin.js cli",
    "db:reset": "npm run prerun && node ./scripts/egg-born-bin.js backend-db-reset --flavor=normal",
    "dev": "npm run prerun && node ./scripts/egg-born-bin.js backend-dev --workers=2 --flavor=normal",
    "dev:one": "npm run prerun && node ./scripts/egg-born-bin.js backend-dev --workers=1 --flavor=normal",
    "test": "npm run prerun && node ./scripts/egg-born-bin.js backend-test --flavor=normal",
    "cov": "npm run prerun && node ./scripts/egg-born-bin.js backend-cov --flavor=normal",
    "tsc": "npm run prerun && tsc -b",
    "build": "npm run prerun && npm run vona :tools:build",
    "start": "node ./scripts/egg-born-scripts.js backend-start --flavor=normal",
    "start:daemon": "node ./scripts/egg-born-scripts.js backend-start --daemon --flavor=normal",
    "stop": "node ./scripts/egg-born-scripts.js backend-stop --flavor=normal",
    "prerun": "npm run vona :tools:deps",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "pnpm": {
    "overrides": {
      "@types/mime": "3.0.4"
    }
  },
  "dependencies": {
    "vona": "workspace:^"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/eslint-parser": "^7.24.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "@babel/register": "^7.23.7",
    "@cabloy/extend": "workspace:^",
    "@cabloy/lint": "workspace:^",
    "@cabloy/module-glob": "workspace:^",
    "@cabloy/process-helper": "workspace:^",
    "@eslint/config-inspector": "^1.0.0",
    "@swc/core": "^1.7.36",
    "@swc/helpers": "^0.5.13",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^22.10.2",
    "@zhennann/socketio": "^4.0.2",
    "async": "^3.2.5",
    "autoprefixer": "^9.8.8",
    "axios": "^1.6.8",
    "bagpipe": "^0.3.5",
    "bluebird": "^3.7.2",
    "chalk": "^3.0.0",
    "commander": "^11.1.0",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^7.0.0",
    "egg-born-utils": "^1.2.0",
    "eslint": "^9.20.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.19.2",
    "extend2": "^1.0.1",
    "fs-extra": "^10.1.0",
    "function-bind": "^1.1.2",
    "glob": "^10.3.12",
    "gogocode": "^1.0.55",
    "http-proxy-middleware": "^1.3.1",
    "is-type-of": "^1.4.0",
    "js-cookie": "^2.2.1",
    "jsdoc": "^4.0.2",
    "json5": "^2.2.3",
    "less": "^3.13.1",
    "mini-css-extract-plugin": "^1.6.2",
    "minimatch": "^9.0.4",
    "moment": "^2.30.1",
    "npm-request": "^1.0.0",
    "open": "^7.4.2",
    "ora": "^4.1.1",
    "p-map": "^3.0.0",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^5.0.5",
    "semver": "^7.6.0",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.7.3",
    "vona-mock": "workspace:^",
    "vue-eslint-parser": "^9.4.2"
  }
}
