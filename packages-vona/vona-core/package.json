{
  "name": "vona-core",
  "type": "module",
  "version": "5.0.7",
  "description": "vona",
  "publishConfig": {
    "access": "public"
  },
  "author": "zhennann",
  "keywords": [
    "framework",
    "cabloy"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@cabloy/compose": "workspace:^",
    "@cabloy/deps": "workspace:^",
    "@cabloy/dotenv": "workspace:^",
    "@cabloy/extend": "workspace:^",
    "@cabloy/json5": "workspace:^",
    "@cabloy/localeutil": "workspace:^",
    "@cabloy/module-info": "workspace:^",
    "@cabloy/module-info-pro": "workspace:^",
    "@cabloy/set": "workspace:^",
    "@cabloy/utils": "workspace:^",
    "@cabloy/word-utils": "^2.0.0",
    "fs-extra": "^10.1.0",
    "koa": "^2.15.4",
    "reflect-metadata": "^0.2.2",
    "retry": "^0.13.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/koa": "^2.15.0",
    "@types/retry": "^0.12.5",
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "scripts",
      "exports.\\..import"
    ]
  }
}
