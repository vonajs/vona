{
  "name": "vona-core",
  "type": "module",
  "version": "5.0.45",
  "description": "vona",
  "publishConfig": {
    "access": "public"
  },
  "author": "zhennann",
  "keywords": [
    "framework",
    "cabloy"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "@cabloy/compose": "^2.0.14",
    "@cabloy/extend": "^3.1.10",
    "@cabloy/json5": "^1.0.18",
    "@cabloy/localeutil": "^2.0.10",
    "@cabloy/module-info": "^1.3.26",
    "@cabloy/module-info-pro": "^1.0.34",
    "@cabloy/set": "^1.0.17",
    "@cabloy/utils": "^1.0.39",
    "@cabloy/word-utils": "^2.0.1",
    "chalk": "^5.3.0",
    "fs-extra": "^10.1.0",
    "koa": "^3.0.0",
    "reflect-metadata": "^0.2.2",
    "retry": "^0.13.1",
    "triple-beam": "^1.4.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.58"
  },
  "devDependencies": {
    "@types/koa": "^2.15.0",
    "@types/retry": "^0.12.5",
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "exports.\\..import"
    ]
  },
  "gitHead": "0eab9dc4a5622caffe89e7b1b3f02c08ccbc4c4b"
}
