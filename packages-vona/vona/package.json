{
  "name": "vona",
  "type": "module",
  "version": "5.0.68",
  "description": "Vona is an intuitive, elegant and powerful Node.js framework for rapidly developing enterprise applications of any size",
  "author": "zhennann",
  "homepage": "https://github.com/vonajs/vona#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vonajs/vona.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/vonajs/vona/issues"
  },
  "keywords": [
    "nodejs",
    "framework",
    "vona"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=24.1.0"
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore",
    "version": "node ./scripts/versionTemplate.ts"
  },
  "dependencies": {
    "vona-core": "^5.0.40",
    "vona-suite-a-vona": "^5.0.63"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "exports.\\..import"
    ]
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "fs-extra": "^11.2.0",
    "rimraf": "^6.0.1"
  },
  "gitHead": "0eab9dc4a5622caffe89e7b1b3f02c08ccbc4c4b"
}
