{
  "name": "vona",
  "version": "5.0.7",
  "description": "vona",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "lint": "eslint ."
  },
  "keywords": [
    "egg",
    "egg-born",
    "framework",
    "cabloy"
  ],
  "author": "zhennann",
  "dependencies": {
    "vona-core": "workspace:^",
    "vona-shared": "workspace:^",
    "vona-suite-a-vona": "workspace:^"
  }
}
