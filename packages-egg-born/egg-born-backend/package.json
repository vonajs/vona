{
  "name": "egg-born-backend",
  "type": "module",
  "version": "10.0.7",
  "description": "egg-born-backend",
  "author": "zhennann",
  "homepage": "https://github.com/zhennann/egg-born-backend#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhennann/egg-born-backend.git"
  },
  "bugs": {
    "url": "https://github.com/zhennann/egg-born-backend/issues"
  },
  "keywords": [
    "egg",
    "egg-born",
    "framework",
    "backend"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@types/uuid": "^9.0.8",
    "egg": "^3.17.5",
    "egg-onerror": "^2.1.1",
    "jsonwebtoken": "^9.0.2",
    "koa-compose": "^4.1.0",
    "koa-jwt2": "^1.0.3",
    "utility": "^1.17.0",
    "uuid": "^11.0.3",
    "vona": "^5.0.7"
  },
  "devDependencies": {
    "@types/koa-cors": "^0.0.6",
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  },
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "scripts",
      "exports.\\..import"
    ]
  }
}
