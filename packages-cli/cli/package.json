{
  "name": "vona-cli",
  "version": "1.0.1",
  "description": "vona cli",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "bin": {
    "vona": "dist/bin/vona.js"
  },
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "clean": "tsc -b --clean",
    "tsc:publish": "npm run clean && tsc -b",
    "prepublishOnly": "npm run tsc:publish"
  },
  "keywords": [
    "framework",
    "vona"
  ],
  "author": "zhennann",
  "license": "MIT",
  "dependencies": {
    "@cabloy/cli": "workspace:^",
    "vona-cli-set-api": "workspace:^"
  }
}
