{
  "name": "vona-cli",
  "type": "module",
  "version": "1.0.82",
  "description": "vona cli",
  "publishConfig": {
    "access": "public"
  },
  "author": "zhennann",
  "license": "MIT",
  "keywords": [
    "framework",
    "vona"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./*": "./*"
  },
  "bin": {
    "vona": "dist/bin/vona.js"
  },
  "files": [
    "dist"
  ],
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "exports.\\..import"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@cabloy/cli": "^3.0.33",
    "@cabloy/process-helper": "^2.0.18",
    "fs-extra": "^11.2.0",
    "vona-cli-set-api": "^1.0.84"
  },
  "devDependencies": {
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  },
  "gitHead": "0eab9dc4a5622caffe89e7b1b3f02c08ccbc4c4b"
}
