{
  "name": "@cabloy/cli",
  "type": "module",
  "version": "3.0.41",
  "description": "@cabloy/cli",
  "publishConfig": {
    "access": "public"
  },
  "author": "zhennann",
  "license": "MIT",
  "keywords": [
    "framework",
    "cabloy-cli"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "clean-package": {
    "indent": 2,
    "remove": [
      "clean-package",
      "devDependencies",
      "exports.\\..import"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "tsc:publish": "npm run clean && tsc",
    "prepublishOnly": "npm run tsc:publish",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@babel/parser": "^7.26.5",
    "@cabloy/module-glob": "^5.2.23",
    "@cabloy/module-info": "^1.3.25",
    "@cabloy/process-helper": "^2.0.18",
    "@cabloy/utils": "^1.0.38",
    "@cabloy/word-utils": "^2.0.1",
    "@npmcli/config": "^8.3.4",
    "@zhennann/common-bin": "^4.0.0",
    "@zhennann/ejs": "^3.0.1",
    "boxen": "^4.2.0",
    "chalk": "^3.0.0",
    "cli-table3": "^0.6.5",
    "enquirer": "^2.3.6",
    "fs-extra": "^11.2.0",
    "globby": "^14.1.0",
    "gogocode": "^1.0.53",
    "is-type-of": "^2.1.0",
    "istextorbinary": "^3.3.0",
    "semver": "^7.6.2",
    "tmp": "^0.2.3",
    "urllib": "^3.24.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "clean-package": "^2.2.0",
    "rimraf": "^6.0.1"
  },
  "gitHead": "0eab9dc4a5622caffe89e7b1b3f02c08ccbc4c4b"
}
