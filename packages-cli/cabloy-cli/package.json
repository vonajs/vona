{
  "name": "@cabloy/cli",
  "type": "module",
  "version": "3.0.0",
  "description": "@cabloy/cli",
  "publishConfig": {
    "access": "public"
  },
  "author": "zhennann",
  "license": "MIT",
  "keywords": [
    "framework",
    "cabloy-cli"
  ],
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "clean": "tsc -b --clean",
    "tsc:publish": "npm run clean && tsc -b",
    "prepublishOnly": "npm run tsc:publish"
  },
  "dependencies": {
    "@babel/parser": "^7.26.5",
    "@cabloy/module-glob": "workspace:^",
    "@cabloy/module-info": "workspace:^",
    "@cabloy/process-helper": "workspace:^",
    "@cabloy/word-utils": "^1.0.8",
    "@npmcli/config": "^8.3.4",
    "@zhennann/common-bin": "^4.0.0",
    "@zhennann/ejs": "^3.0.1",
    "boxen": "^4.2.0",
    "chalk": "^3.0.0",
    "cli-table3": "^0.6.5",
    "egg-born-utils": "^1.2.0",
    "enquirer": "^2.3.6",
    "esbuild": "^0.23.0",
    "fs-extra": "^11.2.0",
    "gogocode": "^1.0.53",
    "is-type-of": "^2.1.0",
    "istextorbinary": "^3.3.0",
    "semver": "^7.6.2",
    "tmp": "^0.2.3",
    "urllib": "^3.24.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4"
  },
  "gitHead": "2d40c063c115b230fdbc89f5a78b4412ebd0dfc9"
}
